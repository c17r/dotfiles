Host github.com
    HostName github.com
    User git
    IdentityFile "~/Box Sync/Sync/ssh keys/id_git"
    IdentitiesOnly yes
    ForwardAgent no

Host bitbucket.org
    HostName bitbucket.org
    User git
    IdentityFile "~/Box Sync/Sync/ssh keys/id_git"
    IdentitiesOnly yes
    ForwardAgent no

Host heroku.com
    HostName heroku.com
    IdentityFile "~/Box Sync/Sync/ssh keys/id_heroku"
    IdentitiesOnly yes
    ForwardAgent no

Host sdf
    HostName sdf.lonestar.org
    User sauerc
    ForwardAgent yes

Host *.endrun.local *.endrun.org
    User christian
    ForwardAgent yes

Host *
    ControlMaster auto
    ControlPath ~/.ssh/tmp/home-%r@%h:%p
    ControlPersist yes
    IdentityFile "~/Box Sync/Sync/ssh keys/id_rsa"
    IdentitiesOnly yes
    ForwardAgent no

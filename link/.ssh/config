Host github.com
    HostName github.com
    User git
    IdentityFile "~/odrive/Box/Sync/ssh keys/id_git"

Host bitbucket.org
    HostName bitbucket.org
    User git
    IdentityFile "~/odrive/Box/Sync/ssh keys/id_git"

Host heroku.com
    HostName heroku.com
    IdentityFile "~/odrive/Box/Sync/ssh keys/id_heroku"

Host sdf sdf.lonestar.org
    HostName sdf.lonestar.org
    User sauerc
    ForwardAgent yes

Host *.endrun.local *.endrun.org
    User christian
    ForwardAgent yes

Host *
    ControlMaster auto
    ControlPath ~/.ssh/tmp/home-%r@%h:%p
    ControlPersist yes
    IdentityFile "~/odrive/Box/Sync/ssh keys/id_rsa"
    IdentitiesOnly yes
    ForwardAgent no
    AddKeysToAgent yes
    UseKeychain yes
